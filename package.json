{
  "company": "PayPal",
  "name": "accessible-html5-video-player",
  "version": "1.0.5",
  "description": "A lightweight HTML5 video player which includes support for captions and screen reader accessibility.",
  "repository": {
    "type": "git",
    "url": "https://github.com/paypal/accessible-html5-video-player"
  },
  "main": "dist/px-video.cjs.js",
  "module": "dist/px-video.esm.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.5.10",
    "rollup": "^0.67.0",
    "sass": "^1.14.3",
    "serve": "^9.4.2"
  },
  "keywords": [
    "html5",
    "accessible",
    "video",
    "player",
    "paypal",
    "javascript"
  ],
  "author": "weboverhauls@gmail.com",
  "licenses": [
    {
      "type": "BSD",
      "url": "https://github.com/paypal/accessible-html5-video-player/blob/master/LICENSE.md"
    }
  ],
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/paypal/accessible-html5-video-player/issues"
  },
  "homepage": "https://github.com/paypal/accessible-html5-video-player#readme",
  "scripts": {
    "build": "npm-run-all -p build:*",
    "build:cjs": "rollup src/px-video.js --file dist/px-video.cjs.js --format cjs",
    "build:esm": "rollup src/px-video.js --file dist/px-video.esm.js --format esm",
    "build:global": "rollup src/px-video.js --file dist/px-video.global.js --format iife --output.name=InitPxVideo",
    "build:react": "babel src/px-video.jsx --out-file dist/px-video.react.js --presets=env,react",
    "build:sass": "sass src/px-video.scss css/px-video.css",
    "demo": "serve .",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
